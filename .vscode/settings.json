{
  "editor.detectIndentation": false,
  "files.readonlyInclude": {
    "LICENSE": true,
    "node_modules/**": true
  },
  "search.exclude": {
    "**/*.code-search": false,
    "**/bower_components": false,
    "**/node_modules": true,
    "LICENSE": true,
    "package-lock.json": true
  },
  "[javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.insertSpaces": false,
    "editor.tabSize": 4
  },
  "[json][yaml]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[markdown]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.wordWrap": "off"
  }
}